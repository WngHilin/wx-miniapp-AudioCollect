<!--pages/info/info.wxml-->
<view class="page-body">
  <view class="page-section">
    <view class="weui-cells__title">修改年龄</view>
    <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_input">
        <input class="weui-input" auto-focus placeholder="请输入年龄"/>
      </view>
    </view>
  </view>
</view>
<form bindsubmit="formSubmit" bindreset="formReset">
  <view class="btn">
    <button size="default" type="primary" formType="submit" bindtap="backward">提交</button>
  </view>
</form>
